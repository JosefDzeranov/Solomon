#include <iostream>
#include <fstream>
#include <math.h>
using namespace std;
int main()
{int a1,a2,a3,a4;
float n;
cin>>a1>>a2>>a3>>a4;
if(a1>a2){a2=a1-a2;a1=a1-a2;}
if(a2>a1){a1=a2-a1;a2=a2-a1;a1=a2;}
if(a3>a4){a4=a3-a4;a3=a3-a4;}
if(a4>a3){a3=a4-a3;a4=a4-a3;a3=a4;}
n=a1+a3;
n=sqrt(n);
a1=n;
cout<<a1;

return 0;}
