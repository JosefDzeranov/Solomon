#include <iostream>

using namespace std;

int main()
{
    
    int n;
    
    cin>>n;
    
    int  mas[n];
    
    int i;
    
    
    
    for(i = 0; i<n; i++)
    {
        cin>>mas[i];
    }
    
    int per = 0; // Значение первоначального массива
    int sump = 0; // Сумма цифр в числе
    int sumb = 0; // для чилел которе больше 9 (произошло от sump)
    int root[n];
    
    for(i = 0; i<n; i++)
    {
        per=mas[i];
        
        while (per!=0)
        {
            sump += per%10;
            per /= 10;
            
            if (sump>9) {
                
                while (sumb!=0) {
                    sumb += sump%10;
                    sump /= 10;
                }
            }
            
        }
        
        
        sumb = sump;
        root[i]= sump;
        sump = 0;
    }
    for (i=0; i<n; i++)
    {
        cout<<root[i]<<' ';
    }
}
