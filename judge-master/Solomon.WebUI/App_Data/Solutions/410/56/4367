#include <cmath>
#include <iostream>

using namespace std;


int main()
{
 double n, chisl = 0, lol = 2, dod; // chisl - колличество нулей
 cin >> n;
 for(double i = 0, j = 0; i <= n; i++, j++) // i - проверяемое число
 {
  if(pow(lol, j) == i)
  {
   chisl = i - 1;            
  }
  
  else if(pow(lol, j) > i)
  {
   chisl = chisl;     
  }
  dod = i;
 }
 if(n == lol - 1)
 {
  chisl = dod - 1;     
 }
 cout << chisl;
}
