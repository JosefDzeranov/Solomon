#include <cmath>
#include <iostream>

using namespace std;


int main()
{
 double n, chisl = 0, lol = 2; // chisl - колличество нулей
 cin >> n;
 for(int i = 0, j = 0; i <= n ; i++, j++)
 {
  if(pow(lol, j) == n)
  {
   chisl += n - 1;            
  }
  else if(i % 2 == 1)
  {
   continue;     
  }
  else if((i % 12 == 0) || (i % 10 == 0))
  {
   chisl = i - 2;     
  }
       
 }
 cout << chisl;
}
