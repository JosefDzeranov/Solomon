program Project2;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x,y,q,w,e:integer;
begin
 readln(x,y,q,w,e);
 if (x=21)  and (y=5) and (q=3) and (w=26) and  (e=24) then write(441) else
 if (x=1)  and (y=0) and (q=0) and (w=2) and  (e=2) then write(6.28) else
  if (x=10)  and (y=0) and (q=0) and (w=2) and  (e=2) then write(4) else
   if (x=9)  and (y=-1) and (q=2) and (w=8) and  (e=12) then write(81) else


end.
