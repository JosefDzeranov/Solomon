using System;
class Velik
{
	static void Main()
	{
		string s = Console.ReadLine();
		int n = int.Parse(s.Split()[0]);
		int k = int.Parse(s.Split()[1]);

		double N = 1D;
		double K = 1D;

		for(int i = 1; i<= n;i++)
			N*= i;
		for(int j = 1; j<= k;j++)
			K*= j;
		Console.Write(N/(K*N-K*K)); 	
	
    }
	
	
}
