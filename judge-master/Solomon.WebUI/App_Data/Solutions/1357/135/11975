#include <iostream>


using namespace std;

int main()
{
    long long A,K,B,M,X,c=0;
    cin>>A>>K>>B>>M>>X;
    while (X>0)
    {
    if (K==0)
        X=X-A;
    else
        {
            if (c%K!=0)
        {
            X=X-A;
        }
        }
        if (M==0)
            X=X-B;
        else
        {


        if (c%M!=0)
        {
            X=X-B;
        }

    }
    c++;
    }
    cout<<c-1;
    return 0;
}
