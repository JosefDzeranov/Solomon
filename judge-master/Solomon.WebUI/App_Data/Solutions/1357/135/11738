#include <iostream>
#include <cmath>
#include <string>
using namespace std;

int main()
{
    long long A,K,B,M,X,c=0,d=0;
    cin>>A>>B>>K>>M>>X;;
    while (X>d)
    {
        if (c%K!=0)
        {
            d=d+A;
        }
        if (c%M!=0)
        {
            d=d+B;
        }
        c++;
    }
    cout<<c-1;
    return 0;
}
