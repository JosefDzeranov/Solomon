using System;
using System.Collections.Generic;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            if (n < 20) Console.WriteLine(0);
            else
                Console.WriteLine((n + 16) / 12);
        }
    }
}
