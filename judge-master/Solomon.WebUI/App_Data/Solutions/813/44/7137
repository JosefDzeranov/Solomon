program mesi8;
var n, l, r : integer; k : real;

begin
read(n, l, r);
    if (l mod (2*r) = 0) then k := (l/(2*r))
      else k := ((l div (2*r)) + 1);
      if 2*r > l then writeln('NO') else 
if ((pi*2*r*k) > n) then writeln('NO')
  else 
    begin
    writeln('YES');
       if (l mod (2*r) = 0) then writeln(l/(2*r))
      else writeln((l div (2*r)) + 1);
    end;
end.