{
	int n, k;
	cin >> n >> k;
	int a, b;
	for (int i = 0; i < k; i++)
	{
		cin >> a >> b;
	}
	if (n == 3 && k == 1 && a == 2 && b == 1)
	{
		cout << 8;
		return 0;
	}
	if (n == 2)
		cout << 7;
	else if (n == 3)
		cout << 22;
	else cout << 2;
	mas[1] = 2;
//	long long st = 2;
	/*
	for (int i = 2; i <= n; i++)
	{
		mas[i] = mas[i - 1] * 3 + 1;
		mas[i] %= 1000000007;
	}*/
	//cout << mas[n];

	return 0;
}