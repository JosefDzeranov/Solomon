#include <iostream>


using namespace std;

int main()
{
	long long a, k, b, m, x;
	cin >> a >> k >> b >> m >> x;
	long long l = 0, r = 1000000003;
	while (l < r - 1)
	{
		long long mid = (l + r) / 2;
		long long t1 = a * (mid - mid / k);
		long long t2 = b * (mid - mid / m);
		long long t = t1 + t2;
		if (t >= x)
			r = mid;
		else
			l = mid;
	}
	l--;
	long long t1 = a * (l - l / k);
	long long t2 = b * (l - l / m);
	long long t = t1 + t2;
	if (t >= x)
		cout << l;
	else
	{
		l++;
		t1 = a * (l - l / k);
		t2 = b * (l - l / m);
		t = t1 + t2;
		if (t >= x)
			cout << l;
		else
		{
			l++;
			t1 = a * (l - l / k);
			t2 = b * (l - l / m);
			t = t1 + t2;
			if (t >= x)
				cout << l;
			else
				cout << l + 1;
		}
	}
	return 0;
}