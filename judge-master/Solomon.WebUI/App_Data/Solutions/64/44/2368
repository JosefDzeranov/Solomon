#include <iostream>

using namespace std;


int main()
{
	int n, l;
	cin >> n >> l;
	int r;
	cin >> r;
	long double a = 2 * 3.141592653589793238462 * r;
	int v;
	v = l / (r * 2);
	if (l % (r * 2) != 0) v++;
	if (v * a <= n)
	{
		cout << "YES" << "\n";
		cout << v;
	}
	else cout << "NO";

	return 0;
}