#include <iostream>
#include <cmath>
#include <string>
#include <algorithm>
#include <vector>
#include <deque> 
#include <map>
#include <stdlib.h>
#include <time.h> 
using namespace std;

int main()
{
	//srand(time(NULL));
	int x1, y1, x2, y2, x3, y3;
	cin >> x1 >> y1 >> x2 >> y2 >> x3 >> y3;
	int ans = min(min(x3 - x1, y3 - y1), min(x2 - x3, y2 - y3));
	cout << ans;
}