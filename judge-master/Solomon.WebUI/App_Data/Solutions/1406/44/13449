#include<stdio.h>

int main()
{   
  //freopen("tur2.in","r",stdin);
  //freopen("tur2.out","w",stdout);
  
  int n,m,k;
  
  scanf("%d%d",&n,&m);
  scanf("%d",&k);

  if(n>=m) 
  {
    k*=2;      
    printf("YES\n");      
    if(m%k==0) k=m/k;  
      else k=m/k+1;
    printf("%d\n",k);  
  }else printf("NO\n");
  
  return 0;  
}


