#include<iostream>
using namespace std;
int main(){
	int a1,a2,a3,a4,a,b,k,i,l;
	freopen("pobeda.in","r",stdin);
		cin>>a1>>a2>>a3>>a4;
	freopen("pobeda.out","w",stdout);

	if(a1<a2)a=a1;
	else a=a2;

	if(a3<a4)b=a3;
	else b=a4;
	k=a+b;

	for(i=1;i<=k/2;i++)
	{
		if(i*i<=k)l=i;
	}
	cout<<l;
	return 0;
}