#include <cstdlib>
#include <iostream>

using namespace std;

long fact(int num);

int main(int argc, char *argv[])
{
    int N;
    //cout << "Input number: ";
    cin >> N;
    
    N = fact(N);
    
    cout << N << endl;
    
    system("PAUSE");
    return EXIT_SUCCESS;
}

long fact(int num)
{
    long res=1;
    for (int i=0; i<3; i++)
    {
        res=res*(num-i);
    }
    return res;   
}
