#include <iostream>
#include <string>
#include <fstream>
#include <stdlib.h>

using namespace std;

int main()
{
   // ifstream fin("river.in");
	//ofstream fout("river.out");
	int md, dv, a, a2, kv, e, v, n, p, k, i, j, ln, n4, f;
	string str, strN, str1;
	char arr[6];

	cin >> n >> p;

	kv = 0;

	for (i = 0; i < n; i++){
        cin >> a;
        kv += a*a;
        itoa(a,arr,10);
        str = arr;
        ln = str.length();
        for (j = 0; j < 6 - ln; j++){
                str = "0" + str;
        }
        strN += str;
	}
	cout << kv << endl;

    cin >> k;

    for (i = 0; i < k; i++){
        cin >> e >> v;
           if (e == 1){
               if (v == 1)
                {
                    str.assign(strN,0,6);
                    for (j = 0; j < str.length(); j++)
                        arr[j] = str[j];
                    a = atoi(arr);
                    str.assign(strN,6,6);
                    for (j = 0; j < str.length(); j++)
                        arr[j] = str[j];
                    a += atoi(arr);


                    itoa(a,arr,10);
                    str = arr;
                    ln = str.length();
                    for (j = 0; j < 6 - ln; j++){
                            str = "0" + str;
                    }
                    strN.replace(0,12,str);
                }
                else if (v == (strN.length() / 6) )
                {
                    str.assign(strN,(v - 2) * 6,6);
                    for (j = 0; j < str.length(); j++)
                        arr[j] = str[j];
                    a = atoi(arr);
                    str.assign(strN,(v - 1) * 6,6);
                    for (j = 0; j < str.length(); j++)
                        arr[j] = str[j];
                    a += atoi(arr);

                    itoa(a,arr,10);
                    str = arr;
                    ln = str.length();
                    for (j = 0; j < 6 - ln; j++){
                            str = "0" + str;
                    }
                    strN.replace((v - 2) * 6,12,str);
                }
                else
                {
                    str.assign(strN,(v - 1) * 6,6);
                    for (j = 0; j < str.length(); j++)
                        arr[j] = str[j];
                    a = atoi(arr);
                    a2 = a;
                    dv = a / 2;

                    str.assign(strN,(v - 2) * 6,6);
                    for (j = 0; j < str.length(); j++)
                        arr[j] = str[j];
                    a = atoi(arr);

                    a += dv;
                    itoa(a,arr,10);
                    str = arr;
                    ln = str.length();
                    for (j = 0; j < 6 - ln; j++){
                            str = "0" + str;
                    }

                    str1.assign(strN,v * 6,6);
                    for (j = 0; j < str1.length(); j++)
                        arr[j] = str1[j];
                    a = atoi(arr);
                    a += a2 - dv;
                    itoa(a,arr,10);
                    str1 = arr;
                    ln = str1.length();
                    for (j = 0; j < 6 - ln; j++){
                            str1 = "0" + str1;
                    }

                    str += str1;
                    strN.replace((v - 2) * 6,18,str);
                }
           }
           else
           {
               str.assign(strN,(v - 1) * 6,6);
               for (j = 0; j < str.length(); j++)
                    arr[j] = str[j];
                a = atoi(arr);
                a2 = a;
                dv = a / 2;
                a = a2 - dv;


                itoa(dv,arr,10);
                str = arr;
                ln = str.length();
                for (j = 0; j < 6 - ln; j++){
                        str = "0" + str;
                }

                itoa(a,arr,10);
                str1 = arr;
                ln = str1.length();
                for (j = 0; j < 6 - ln; j++){
                        str1 = "0" + str1;
                }

                str += str1;
                strN.replace((v - 1) * 6, 6,str);
           }
           kv = 0;
           for (j = 0; j < strN.length() / 6; j++)
           {
               str.assign(strN, j * 6, 6);
               for (f = 0; f < str.length(); f++)
                    arr[f] = str[f];
                a = atoi(arr);
                a *= a;
                kv += a;
           }
           cout << kv << endl;
    }
	return 0;
}
