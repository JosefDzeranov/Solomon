var k, i, N, s:integer;
begin
     readln(N);
     while (N<1) or (N>10000) do
        readln(N);
     k:=0;
     s:=0;
     for i:=1 to N do
     begin
          k:=i;
          while k mod 2=0 do
          begin
               k:=k div 2;
               s:=s+1;
          end;
     end;
     writeln(s);
     read(i);
end.
