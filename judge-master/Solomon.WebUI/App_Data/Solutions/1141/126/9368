Program a2;4
uses SysUtils;
var n,c,i,k: integer;
s: real;
a: array [1..50] of integer;
label t1, t2;
begin
writeln('введите количество шкафов');
readln(n);
writeln ('введите количество наград в каждом шкафу');
for i:=1 to n do
readln (a[i]);
c:=0;
for i:=1 to n do
c:=c+a[i];
s:=c/n;
if c mod n <> 0 then goto t1;
k:=0;
for i:=1 to n do 
if a[i]<>s then
k:=k+1;
writeln (k);goto t2;
t1: writeln ('IMPOSSIBLE');
t2:end.