program Project4;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x,y,x1,y1,x2,y2,x3,y3: integer;
begin
  readln(x,y,x1,y1,x2,y2,x3,y3);
  if (x=x1) and (y=y2) and (x2 =x3) and (y1=y3) and ((x-x1)=(x2-x3)) and ((y-y2)=(y2-y3)) and ((x-x1)=(y-y2))   then
   writeln('YES')
   else
   writeln('NO');
   readln;
end.
