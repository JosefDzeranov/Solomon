program Project4;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x,x1,x2,x3,x4,y,y1,y2,y3,y4,c,d,f,e: integer;
begin
 readln(x1,x2,x3,x4,y1,y2,y3,y4);
  x:=(x1+x2+x3+x4) div 4;
  y:=(y1+y2+y3+y4) div 4;
  if (abs(x-x1) + abs(x-x2) + abs(x-x3) + abs(x-x4))=(abs(y-y1) + abs(y-y2) + abs(y-y3) + abs(y-y4)) then
  if (((x-x1) + (x-x2) + (x-x3) + (x-x4)) and ((y-y1) + (y-y2) + (y-y3) + (y-y4)))=0 then
    writeln('YES')
    else
    writeln ('NO');
    readln;
end.
