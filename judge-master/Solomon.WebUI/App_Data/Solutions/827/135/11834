#include <iostream>
#include <vector>

using namespace std;

int main()
{
	unsigned long long a, k, b, m, x;
	cin >> a >> k >> b >> m >> x;
	unsigned long long d1 = 2, d2 = 1000000000000000000, d;
	bool p = true;
	unsigned long long rez = 0;
	while (p){
		d = (d2 - d1)/2;
		if (x > (a*d - (d / k)*a) + (b*d - (d / m)*b))
			d1 = d;
		else if (x < (a*d - (d / k)*a) + (b*d - (d / m)*b))
			d2 = d;
		else if (x == (a*d - (d / k)*a) + (b*d - (d / m)*b))
			rez = (a*d - (d / k)*a) + (b*d - (d / m)*b);
		if (d <= 10000000) p = false;
	}
	if (rez==0)
	for (unsigned long long int i = d1; i < d2; i++)
	if ((a*i - (i / k)*a) + (b*i - (i / m)*b) >= x){
		rez = i;
		break;
	}
	cout << rez;
	return 0;
}