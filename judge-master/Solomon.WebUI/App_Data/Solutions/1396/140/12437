Module Module1

    Sub Main()
        Dim N, K, I, S, L As Integer
        B , A(0;N) As String
        K = Val(Text1.text)
        S = 0
        B = text2.Text
        N = Len(B)
        For I = 1 To N
            K = O
            Do
                A(I) = Mid(A, I, 1)
                If A(I) = "P" Then S = S + 1 : L = I
