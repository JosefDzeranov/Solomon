Module Module1

    Sub Main()
        Dim K, N, M, I, j As Byte, B(N, M) As Byte
        InputBox(N)
        InputBox(M)
        K = 0
        For I = 1 To N
            For j = 1 To M
                InputBox(B(I, j))
                If B(I, j) = 0 And B(I, j - 1) = 0 Then K = K + 1
                If B(I, j) = 0 And B(I - 1, j) = 0 Then K = K + 1

            Next j

        Next I
        Print(K)

    End Sub

    Private Function B(ByVal p1 As Integer, ByVal j As Byte) As Object
        Throw New NotImplementedException
    End Function

    Private Function Text2() As Object
        Throw New NotImplementedException
    End Function

    Private Function Text1() As Object
        Throw New NotImplementedException
    End Function

    Private Sub B(ByVal p1 As Integer)
        Throw New NotImplementedException
    End Sub

    Private Function Form1() As Object
        Throw New NotImplementedException
    End Function

End Module

