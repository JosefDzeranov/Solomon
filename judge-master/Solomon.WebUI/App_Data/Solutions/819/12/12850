n, k = map(int, input().split())
summa = 0
dividers = list()
powers = list()

# список делителей основания системы счисления
for i in range (2, k//2+1):
    if k % i == 0:
        dividers.append(i)

# степени множителей, не превышающие n
for i in dividers:
    power = 0
    multiplier = i
    while multiplier <= n:
        multiplier *= i
        power += 1
    powers.append(power)

# сколько раз встречаются множители и их степени
for i in range (1, n+1):
    for j in range (1, len(dividers)):
        if i % dividers[j] == 0:
            summa += 1

if len(dividers) == 1:
    start = 0
else:
    start = 1

for i in range (start, len(powers)):
    for j in range (2, powers[i]+1):
        summa += n//dividers[i]**j
        #print (summa)

    
#print (dividers)
#print (powers)
print (summa)
    