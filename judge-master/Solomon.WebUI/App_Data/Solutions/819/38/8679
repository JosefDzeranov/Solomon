x1, y1 = map(int, input().split())
x2, y2 = map(int, input().split())
x3, y3 = map(int, input().split())
x4, y4 = map(int, input().split())
answer = 'NO'
X = [x1, x2, x3, x4]
Y = [y1, y2, y3, y4]
X.sort()
Y.sort()
if (X[3]-X[2] == Y[3]-Y[2]):
    if (X[2]-X[1] == Y[2]-Y[1]):
        if (X[1]-X[0] == Y[1]-Y[0]):
            answer = 'YES'
print (answer)