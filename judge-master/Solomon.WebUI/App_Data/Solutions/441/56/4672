var s, s2, n:string;
i,u,  a, n1, n2, s1, k1, cnt:integer;

begin
readln(a);
 begin
 n1:=a;
 s1:=1;
 k1:=1;
 while k1<n1 do
  begin
  k1:=k1+1;
  s1:=s1*k1;
  end;
end;

begin
n2:=s1;
while n2>0 do begin
insert(chr(48+n2 mod 2),s2,1);
n2:=n2 div 2;
end;
end;

i:=length(s2)+1;
repeat
i:=i-1;
if s2[i] = '0'
then   cnt:=cnt+1 
until s2[i]<>'0';
writeln(cnt);
end.
