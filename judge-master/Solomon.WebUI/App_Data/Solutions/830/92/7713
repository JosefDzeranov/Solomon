program Safaripark;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x1, y1, x2, y2, x3, y3, l1, l2, l3, l4, a, b:integer;
begin
readln (x1,y1);
readln (x2,y2);
readln (x3,y3);
l1:=x3-x1;
l2:=y3-y1;
l3:=y2-y3;
l4:=x2-x3;
if l1<l2 then
a:=l1 else
a:=l2;
if l3<l4  then
b:=l3 else
b:=l4;
if a<b then
writeln (a) else
writeln (b);
readln;
end.
