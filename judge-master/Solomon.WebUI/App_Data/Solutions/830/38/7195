program Project5;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x1, y1, x2, y2, x3, y3, x4, y4, l, h, s:byte;


begin
readln (x1, y1);
readln (x2, y2);
readln (x3, y3);
readln (x4, y4);
l := x2 - x1;
h := y1 - y3;
if (l = h ) then
writeln ('YES')
else writeln ('NO');
readln;

  { TODO -oUser -cConsole Main : Insert code here }
end.
