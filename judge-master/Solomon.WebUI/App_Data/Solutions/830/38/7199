program Project5;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x1, y1, x2, y2, x3, y3, x4, y4, l, h, l2, h2, s:byte;


begin
readln (x1, y1);
readln (x2, y2);
readln (x3, y3);
readln (x4, y4);
l := x2 - x1;
h := y1 - y3;
l2 := x4 - x3;
h2 := y2 -y4;
if (l = h ) and (l2 = h2) and (l = l2) and (l * l = h * h) then
writeln ('YES')
else writeln ('NO');
readln;

  { TODO -oUser -cConsole Main : Insert code here }
end.
