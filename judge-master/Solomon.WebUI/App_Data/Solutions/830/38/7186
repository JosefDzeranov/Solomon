program Project5;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x1, y1, x2, y2, x3, y3, x4, y4, l, h:byte;


begin
readln(x1, y1, x2, y2, x3, y3, x4, y4);
l := x2 - x1;
h := y1 - y3;
if (y1 = y2) and (y3=y4) and (x1=x3) and (x2=x4) and (l = x4 - x3) and (h = y2 - y4) then
writeln ('Yes')
else writeln ('No');
readln;

