#include <iostream>

using namespace std;

int main()
{
	int mas[103][103];
	int m, n, k;
	cin >> m >> n >> k;
	for (int i = 0; i <= m; i++)
		for (int j = 0; j <= n; j++)
		{
			mas[i][j] = 0;
		}
	for (int i = 0; i < k; i++)
	{
		int a, b;
		cin >> a >> b;
		mas[a][b] = 1;
	}
	int cnt = 0;
	for (int i = 1; i <= m; i++)
		for (int j = 1; j <= n; j++)
		{
			if (mas[i][j] == 1) continue;
			int fl = 0, fl2 = 0;
			if (i > 1)
			{
				fl++;
				if (mas[i - 1][j] == 1) 
					fl2++;
			}
			if (j < n) 
			{
				fl++;
				if (mas[i][j + 1] == 1)
					fl2++;
			}
			if (i < m) 
			{
				fl++;
				if (mas[i + 1][j] == 1)
					fl2++;
			}
			if (j > 1)
			{
				fl++;
				if (mas[i][j - 1] == 1)
					fl2++;
			}
			if (fl2 == fl) cnt++;
			mas[i][j] = 1;
		}
		if (cnt == 0) cnt = 1;
		cout << cnt;
		return 0;
}