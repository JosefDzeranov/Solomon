s = input()
pointer = 0
res = ""
if s[0] != "+" and s[0] != "-" and s[0] != "#":
    pointer = -1
for i in s:
    if i == "+":
        pointer += 1
    else:
        if i == "-":
            pointer += -1
        else:
            if i == "#":
                res = res[0:pointer:1] + res[pointer + 1::1]
                pointer += -1
            else:
                res += i
                pointer += 1
    if pointer < 0:
        pointer = 0
print(res)
    
        