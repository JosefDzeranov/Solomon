program B;
Uses crt;
var k,n,t,x,y,a:integer;
begin
writeln('Всего у Змея Горыныча 3 головы');
x:=3;
Writeln('Введите количество ударов, которые может нанести Иван');
read(t);
writeln('Введите количество голов, которые срубает Иван одним ударом');
read(n);
writeln('Введите количество голов, которые восстанавливает Змей Горыныч после удара');
read(k);
if n<=k then writeln('No')
else writeln('Yes');
end.