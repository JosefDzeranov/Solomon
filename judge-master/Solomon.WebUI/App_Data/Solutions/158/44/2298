#include <iostream>
#include <math.h>
using namespace std;

int main()
{
    int n,l,x;
    float r1,pi;
    cin>>n>>l>>r1;

    pi=3.1415926;
    x=ceil(l/(2*r1));

        if ((2*r1*x>=l)&&(2*pi*r1*x<=n)){
    cout <<"YES"<< endl<< x ;
        }
    else
    cout <<"NO"<< endl;{
        }
    return 0;
}
