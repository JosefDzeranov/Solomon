var mas:array [1..10000,1..10000] of char;
m:array [1..1000000] of char;
i,j,n,t:longint;
c:char;
st:string;
begin
readln(st);
t:=1;
for i:=1 to round(sqrt(length(st))) do
for j:=1 to round(sqrt(length (st))) do 
begin
mas[i,j]:=st[t];
t:=1+t;
end;

for i:=1 to round(sqrt(length(st))) do
for j:=1 to round(sqrt(length(st))) do 
begin
mas[j,i]:=st[t];
end;

end.