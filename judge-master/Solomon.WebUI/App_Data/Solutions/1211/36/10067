#include <iostream>

using namespace std;

int main()
{
    /*
     * 1 2 3
     * 2 3 4
     * 3 4 5
     * 4 5 6
     * 5 6 7
     */

    int max;

    cin >> max;

    for( int i = 1; i <= max - 2; i++ )
    {
        for( int j = i; j < i + 3; j++ )
        {
            if( j != 1 )
                cout << " ";

            cout << j;
        }
    }

    cout << endl;

    return 0;
}

