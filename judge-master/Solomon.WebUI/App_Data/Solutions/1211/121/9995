#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main()
{
    int side, cells;
    float sq;
    string can;

    scanf( "%d%d", &side, &cells );

    sq = sqrt( side * side - cells );
    can = "No";

    if( (int)sq == sq )
        can = "Yes";

    cout << can << endl;

    return 0;
}

