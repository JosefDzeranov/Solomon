Program konfeti;
var N, a, b, c, d: longint;
Begin
readln(N, a, b, c);
if a+b+c = N then
writeln (a, ' ', b, ' ', c)
else
if a+b+c <= 2*N
then 
writeln (a,' ', b, ' ',  N-a-b)
else
if a+b+c = N
then
writeln (a, ' ', b, ' ', c)
end.
