program Project3;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var a:array[1..100] of integer;
  I,s,n,g,d:integer;
begin
readln(n);
for I := 1 to n*2 do
  begin
    read (a[I]);
  end;
  s:=abs(a[1]-a[3])*abs(a[4]-a[6]);
  g:=abs(a[1]-a[3])*2+abs(a[4]-a[6])*2;
  d:=s+1-g div 2;
  writeln(d);
  readln;
  readln;
end.
