using System;
using System.Collections.Generic;
using System.Text;

namespace ConsoleApplication6
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = Convert.ToInt16(Console.ReadLine());
            int D = Convert.ToInt32(Console.ReadLine());
            int a = D - N;
            int k = 1;
            Console.WriteLine(k + " " + a);
            Console.ReadKey();
        }
    }
}
