using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            string s = Console.ReadLine();
            int l = s.Length;
            int x=1;
            int a=0;
            for (int i = l-1; i >=0; i--)
            {
                if(s[i]=='*')
                    a += x;
                x*=2;
            }
            Console.WriteLine(a);
        }
    }
}
