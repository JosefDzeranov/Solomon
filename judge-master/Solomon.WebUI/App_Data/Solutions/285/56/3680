program olimp2;
var N, a, k, x: integer;
    p: longint;
begin
  p:=1;x:=0;
  readln(N);
  if (N>=1) and (N<=10000) then begin
    for a:=1 to N do p:=p*a;
  
      while (p>0) and (x=0) do begin 
      x:= p mod 2;
      p:= p div 2;
      if x=0 then inc(k);
      end;
  
  writeln(k);
  end 
  else write('Неверно');
  
  end.  