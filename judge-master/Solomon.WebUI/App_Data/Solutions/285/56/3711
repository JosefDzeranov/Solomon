program olimp2;
var N, i, k, x, p: integer;
begin
  repeat
  readln(N)
  until (N<=10000) and (N>=1);
  
  for i:=1 to N do 
      if i mod 2 = 0 then begin
         p:=i;
        while p<>0 do begin
            x:= p mod 2;
            p:= p div 2;
            if x=0 then inc(k);
                
        end;
     end;
       
  writeln(k);
  end.  