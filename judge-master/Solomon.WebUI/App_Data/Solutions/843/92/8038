#include<iostream>
#include<math.h>
#include<stdio.h>
using namespace std;
int main ()
{
	int x1,x2,x3,y1,y2,y3;
	cin>>x1>>y1>>x2>>y2>>x3>>y3;
	if(abs(x1-x3)<abs(x2-x3)) cout<<abs(x1-x3);
	else if(abs(x2-x3)<abs(x1-x3))cout<<abs(x2-x3);
	else if(abs(y1-y3)<abs(y2-y3))cout<<abs(y1-y3);
		      else if(abs(y2-y3)<abs(y1-y3)) cout<<abs(y2-y3);
	return 0;
}