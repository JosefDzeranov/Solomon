#include <conio.h>
#include <stdio.h>
#include <math.h>


int main(){
    int N,step,count = 0, ch;
    //debug
    
    scanf("%d", &N);   
    for(int i = 1; i <= N; i++){       
        ch = i; 
        while(ch%2 == 0){              
            count++;
            ch/=2;
        }
    }
    printf("%d", count);
    return 0;    
}
