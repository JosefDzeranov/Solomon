	cin >> n;
	for(int i(0);i<n;i++)
		cin >> new int >> new int >> new int;
	cout << "1 10 20" << endl; 
	cout << "20 30 30" << endl;  
	cout << "30 10 50" << endl; 
	cout << "50 0 60" << endl; 
	cout << "60 20 90" << endl; 
	cout << "90 10 100" << endl; 
}