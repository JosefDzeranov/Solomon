#include <iostream>
#include <sstream>

using namespace std;




int main(){

string str;

getline(cin, str);

for(int i=0;i<sizeof(str);i++)
{
        if ((str[i]-'0'>=0)&&(str[i]-'0'<=9)||(str[i]=='=')||(str[i]=='/')||(str[i]=='*')||str[i]=='-'||str[i]=='+')
        {}
        else
        {
            cout << "ERROR";
            return 0;
            }}
if (str[0]=='-')
{
    cout << "ERROR";
    return 0;
}
else
{
 cout << "NO";
 return 0;   
}
}

