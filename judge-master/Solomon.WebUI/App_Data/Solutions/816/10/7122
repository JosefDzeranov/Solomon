function Factorial(n: extended): extended;
begin
  if n > 1 then
    Factorial := n * Factorial(n - 1)
  else
    factorial := 1;
end;
 
var
  a, b: extended;
  s: extended;
  ch, zn: extended;
begin
  readln(a,b);
  s := Factorial(a) / (Factorial(a - b) * Factorial(b));
  Writeln(Trunc(s));
  Readln;
end.