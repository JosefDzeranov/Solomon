#include <conio.h>
#include <iostream>
#include <math.h>
#include <string>
using namespace std;
int main()
{
	int k=0;
	string s;
	cin>>s;
	for(int j=s.size()-1;j>=0;j--)
		if (s[j]=='*') k+=pow(2,s.size()-j-1);
	cout<<k;
	//for(int i=k-1;i>=0;i--)
	//{	if (ch[i]=='*') s+=pow(2,k-1-i);
//cout<<s;
	//}		
	//cout<<s;
	return 0;
}