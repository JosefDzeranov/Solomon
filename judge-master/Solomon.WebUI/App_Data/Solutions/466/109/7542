using System;
using System.Collections.Generic;
using System.Text;

namespace _5
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            string s = "";
            for (int i = 0; i < n; i++)
            {
                 s = Console.ReadLine();
            }
            Console.WriteLine( n-1 );
        }
    }
}
