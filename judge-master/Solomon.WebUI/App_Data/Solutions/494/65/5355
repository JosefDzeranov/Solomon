#include <iostream>
using namespace std;

int main()
{
    int a[4];
    for(int i=0;i<4;i++)
    {
        cin >> a[i];
    }
    int kv=0;
    while((a[0]!=0)&&(a[1]!=0))
    {
        kv++;
        a[0]--;
        a[1]--;
    }
    while((a[2]!=0)&&(a[3]!=0))
    {
        kv++;
        a[2]--;
        a[3]--;
    }
    if (kv!=0){
    kv=kv/sqrt(kv);
    }
  cout << kv;
}

