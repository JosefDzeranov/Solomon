#include <iostream>
#include <math.h>

using namespace std;

int main()
{int a1,a2,a3,a4;
float n;
cin>>a1>>a2>>a3>>a4;
if (a1>a2)
 {a2=a1-a2;a1=a1-a2;}
 if (a2>a4)
 {a1=a2-a1;a2=a2-a1;a1=a2;}
 if (a3>a4)
 {a4=a4-a3;a3=a4-a3;}
 if (a4>a3)
 {a3=a3-a4;a4=a4-a3;a3=a4;}
 n=a3+a1;
 n=sqrt(n);
 a1=n;

    cout << a1 << endl;
    return 0;
}
