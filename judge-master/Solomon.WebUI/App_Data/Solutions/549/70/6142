#include <iostream>

using namespace std;

int main()
{
   int_fast64_t n,i,m,j,S=0,minn=0;
   cin>>n;
   int a[n],b[n],c[n],N[n],minn2[n];
   for(i=0;i<n;++i)
    cin>>a[i];
    cin>>m;
     for(j=0;j<m;++j){
    cin>>b[j];
    cin>>c[j];}
     for(j=0;j<n;++j)
        {
                for(i=0;i<m;++i)
        {
           if(b[i]>=a[j])
           {



for(i=0;i<n;++i){
     for(j=0;j<m;++j){
         minn=c[i];
     if(c[i]>c[i+1])
        minn=c[i+1];
     }
    S+=minn;
     }

               }


           }

            }





    cout<<S;
    return 0;
}
