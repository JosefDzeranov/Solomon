  begin
   readln(ma[i,1],ma[i,2],ma[i,3]);
   ma[i,4]:=1;
   ma[i,5]:=0;
  end;
while(s<m) do
 begin
  inc(t);
  for i:=1 to n do
   begin
    if ma[i,4]=t then
     if t mod ma[i,1] then 
   end;
 end;

end.
