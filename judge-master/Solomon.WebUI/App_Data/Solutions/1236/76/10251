program baran;
var a:string;
s,i,q,t:integer;
begin
Readln(i);
q:=0;
for s:=1 to i do
begin
Readln(a);
for t:=1 to i do
begin
if (pos('1',a)<>0) then q:=q+1;
if (pos('2',a)<>0) then q:=q+2;
if (pos('3',a)<>0) then q:=q+3;
if (pos('4',a)<>0) then q:=q+4;
if (pos('5',a)<>0) then q:=q+5;
if (pos('6',a)<>0) then q:=q+6;
if (pos('7',a)<>0) then q:=q+7;
if (pos('8',a)<>0) then q:=q+8;
if (pos('9',a)<>0) then q:=q+9;
end;
end;
Writeln(q div i);
end.
