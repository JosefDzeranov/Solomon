program Project2;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var l,x1,x2,y1,y2:integer;
begin
 readln(l);
read(x1,y1);
readln;
read(x2,y2);
readln;

if (l>(x2-x1)) and (l>(y2-y1)) then writeln((x2-x1)*(y2-y1));
if (l<=(x2-x1)) and (l<=(y2-y1)) then writeln(trunc(3.1415926*l*l));

readln; { TODO -oUser -cConsole Main : Insert code here }
end.
 