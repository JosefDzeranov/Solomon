program Project1;                       //задача 2

{$APPTYPE CONSOLE}

uses
 math, SysUtils;

var a,b,k,m:integer ;
x,t:real;
begin
  read(a);
  read(k);
  read(b);
  read(m);
  readln(x);
  t:=round(x*k*m/((a+b)*k*m-m*a-b*k))-2;
  while ((a+b)*t-(round(t) div k)*a-(round(t) div m)*b)<x do t:=t+1;
  writeln(round(t));
  readln;
end.
