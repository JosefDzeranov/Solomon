program Project1;                       //задача 2

{$APPTYPE CONSOLE}

uses
 math, SysUtils;

var a,b,k,m,t:integer ;
x:longint;
begin
  read(a);
  read(k);
  read(b);
  read(m);
  readln(x);
  t:=round(x/(a+b))-1;
  while ((a+b)*t-(t div k)*a-(t div m)*b)<x do t:=t+1;
  writeln(t);
  readln;
end.
