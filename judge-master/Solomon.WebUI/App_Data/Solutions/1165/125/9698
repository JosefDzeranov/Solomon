#include<iostream>
#include<cmath>
using namespace std;
main()
{
      int x1,y1,x2,y2,x3,y3;
      cin>>x1>>y1>>x2>>y2>>x3>>y3;
      if(y3<y1 && x3<x2 && x3>x1) cout<<abs(y1-y3);
      if(y3>y2 && x3<x2 && x3>x1) cout<<abs(y3-y2);
      if(x3>x2 && y3<y2 && y3>y1) cout<<abs(x3-x2);
      if(x3<x1 && y3<y2 && y3>y1) cout<<abs(x1-x3);
      cin.get(); cin.get();
}
