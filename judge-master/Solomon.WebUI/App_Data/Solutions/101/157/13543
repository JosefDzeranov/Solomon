using System;
using System.Collections.Generic;

using System.Text;

namespace _3
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int sum = 0;
            for (int i = 1; i <= n; i++)
            {
                sum += i;
            }
            if (sum % 2 == 0) Console.WriteLine("Aslan");
            else Console.WriteLine("Timur");
        }
    }
}
