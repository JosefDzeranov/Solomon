using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace _3
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            if (n % 2 == 0) Console.WriteLine("Timur");
            else
                if (n == 1) Console.WriteLine("Timur");
                else Console.WriteLine("Aslan");
        }
    }
}
