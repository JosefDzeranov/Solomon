using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace С
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            long day = n * 30 + n * 7 + n + n / 24;
            Console.WriteLine(n+day/360);
        }
    }
}
