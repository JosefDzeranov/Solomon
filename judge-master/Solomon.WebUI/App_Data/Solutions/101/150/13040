using System;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            string s = Console.ReadLine();
            ulong answer = 0;
            
            for (int i = 0; i < s.Length; i++)
            {
                if (s[s.Length - 1 - i] == '*')
                {
                    answer += Convert.ToUInt64(Math.Pow(2, i));
                }
            }
            Console.WriteLine(answer);
        }
    }
}
