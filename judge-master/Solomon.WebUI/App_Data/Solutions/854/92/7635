program Project1;

{$APPTYPE CONSOLE}

uses
  SysUtils;
var x1,y1,x2,y2,x3,y3,a,b,c,d,men:integer;
begin
 men:=0;
 readln(x1,y1);
 readln(x2,y2);
 readln(x3,y3);
 a:=abs(x3-x1);
 b:=abs(y2-y3);
 c:=abs(x2-x3);
 d:=abs(y3-y1);
 if a<b then men:=a
 else men:=b;    
 if men>c then men:=c;
 if men>d then men:=d;
 writeln(men);
 readln;
end.
