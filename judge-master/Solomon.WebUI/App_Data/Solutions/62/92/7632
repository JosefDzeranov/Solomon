program Project2;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var a,b,c,d,x1,x2,x3,y1,y2,y3:integer;
begin
readln(x1,y1);
readln(x2,y2);
readln(x3,y3);
a:=x3-x1;
b:=x2-x3;
c:=y3-y1;
d:=y2-y3;
if (a<=b) and (a<=c) and (a<=d) then
 writeln(a)
 else
  if (b<=a) and (b<=c) and (b<=d) then
   writeln(b)
  else
   if (c<=a) and (c<=b) and (c<=d) then
   writeln(c)
   else
    if (d<=a) and (d<=b) and (d<=c) then
     writeln(d);
readln;

  
end.
