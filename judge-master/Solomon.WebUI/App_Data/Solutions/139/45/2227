#include <iostream>
#include <cmath>

using namespace std;
//задание № 1
int main()
{
    int n,l,k,len_k;
    int r1,r2;
    cin >> n >> l >> r1 >> r2;
    if (l%(2*r1) > 0)
    {
        k = (l/(2*r1))+1;
    }
    else
    {
        k = (l/(2*r1));
    }
    if (round(4*3.14125*r1) < 4*3.14125*r1)
    {
    len_k=round(4*3.14125*r1)+1;
    }
    else
    {
      len_k=round(4*3.14125*r1);
    }
    if (4*3.14125*r1*k>n)
    {
        cout << "NO";
    }
    else
    {
        cout << "YES" << endl;
        cout << 2*k << " " << k;
    }

    return 0;
}










































