   t=a+b;
   if (v>1000)
   {
       v=x/max(a,b);
   }

 if (v>1000)

{
    while(v>1000)
    {


    v=x/t;
    t=t+b;
if(v>1000)
{
    while(v>1000)

{


        t=t+a;
}
}
}
}
    cout <<v;
