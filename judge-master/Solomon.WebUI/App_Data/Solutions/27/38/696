#include <iostream>
#include <math.h>

using namespace std;

int main()
{
   float d1,Ax,Bx,Cx,Dx,Ay,By,Cy,Dy,d2,d3,d4,d5,d6;
   cin>>Ax>>Ay>>Bx>>By>>Cx>>Cy>>Dx>>Dy;
   d1=sqrt((Ax-Bx)*(Ax-Bx) + (Ay-By)*(Ay-By));
   d2=sqrt((Bx-Cx)*(Bx-Cx) + (By-Cy)*(By-Cy));
   d3=sqrt((Cx-Dx)*(Cx-Dx) + (Cy-Dy)*(Cy-Dy));
   d4=sqrt((Dx-Ax)*(Dx-Ax) + (Dy-Ay)*(Dy-Ay));
   d5=sqrt((Ax-Cx)*(Ax-Cx) + (Ay-Cy)*(Ay-Cy));
   d6=sqrt((Bx-Dx)*(Bx-Dx) + (By-Dy)*(By-Dy));

   if(d1==d2 && d1==d3 && d1==d4 && d5==d6)
   {
       cout<<"YES";

   }
   else
   {
       cout<<"NO";
   }
    return 0;
}
