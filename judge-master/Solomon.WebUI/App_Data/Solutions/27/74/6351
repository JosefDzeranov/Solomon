#include <iostream>

using namespace std;

int main()
{
 int x1,y1,x2,y2,X,Y,m;
 cin>>x1>>y1>>x2>>y2;
 if(x1>x2)
X=x1-x2;
 if(x1<x2)
X=x2-x1;
 if(y1>y2)
Y=y1-y2;
if(y1<y2)
Y=y2-y1;
if(X<100 && Y<100)
    m=1000;
   if(X>100 && Y<100)
    {
        m=(X+Y+(X-100)+(100-Y))*5;
    }
      if(X<100 && Y>100)
    {
        m=1000;
    }

cout<<m;
    return 0;
}
