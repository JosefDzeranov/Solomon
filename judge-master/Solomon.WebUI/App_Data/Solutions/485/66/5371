program Project3;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var s,s2,s3 : AnsiString;
var n,i,sym,x : integer;
var school : array [1..1000] of integer;
begin
readln(n);
for i:= 1 to n do
begin
readln(s2);
s:= s + s2;
s2:= '';
end;
for  i:=1 to 1000 do
s3:= IntToStr(i);
if(Pos(s,s3)<= 5) and (Pos(s,s3) >= 1)  Then
begin
sym:= sym + 1;
school[x]:= i;
x:= x + 1;
end;
writeln(sym);
for i:= 0 to x do
writeln(school[i]);
readln;
end.
