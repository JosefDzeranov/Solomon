#include <iostream>
#include <cstdio>

using namespace std;

int n;

int main() {
    //freopen("in.txt", "r", stdin);
    //freopen("casting.out", "w", stdout);
    cin >> n;

    if (n == 2)
        cout << "2 2";
    else if (n == 3)
        cout << "1 2 3";
    else if (n == 4)
        cout << "1 1 2 4";
    else if (n == 5)
        cout << "1 1 2 2 2";
    else
        cout << -1;

    return 0;
}
