#include<stdio.h>
#include<iostream>
#include<string>
using namespace std;
int main(){
string st,ur;
cin>>st;
int a,b,c,i,d,sch;
sch=st.length()-1;
int fin=0;
a=0;
i=0;
b=0;
i=st.length()-1;
while(b<i){
	a++;
	b=a*a;
	//if(a>i)
		//break;
}
char mas[a][a];
for(i=0;i<a;i++){
	for(c=0;c<a;c++)
	{
		if(fin<=sch)
			mas[i][c]=st[fin];
			//cout<<mas[i][c];
			
		
		if(fin>sch)
			mas[i][c]=' ';
		fin++;
		//	cout<<mas[i][c];	
		}
		
	}

a=0;
for(i=0;i<sch-1;i++){
	for(c=0;c<sch-1;c++){
	ur[a]=mas[c][i];
	cout<<ur[a];
	a++;
	}
}
	return 0;
}