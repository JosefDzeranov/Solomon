#include <iostream>
#include <cmath>
#include <math.h>
using namespace std;

int main()
{float a1,b1,a2,b2,a3,b3,a4,b4,d1,d2,x1,x2,x3,x4,d1,d2;
x1=sqrt((a1-a2)*(a1-a2)+(b1-b2)*(b1-b2));
x2=sqrt((a3-a2)*(a3-a2)+(b3-b2)*(b3-b2));
x3=sqrt((a3-a4)*(a3-a4)+(b3-b4)*(b3-b4));
x4=sqrt((a1-a4)*(a1-a4)+(b1-b4)*(b1-b4));
d1=sqrt((a1-a2)*(a1-a2)+(b1-b2)*(b1-b2));
d2=sqrt((a1-a2)*(a1-a2)+(b1-b2)*(b1-b2));
if(x1==x2&&x2==x3&&x3==x4&&d1==d2&&x4<d2)
cout<<yes;
else cout<<no;
    return 0;
}
