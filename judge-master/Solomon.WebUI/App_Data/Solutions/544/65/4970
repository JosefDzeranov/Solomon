#include <iostream>
#include <cmath>
#include <algorithm>
using namespace std;

int main() {
	long long int a, b, c, d;
	cin >> a >> b >> c >> d;

	long long int fir, sec;

	if (a < b)
		fir = a;
	else
		fir = b;
	if (c < d)
		sec = c;
	else
		sec = d;

	long long int ans = sqrt(double(fir + sec));

	cout << ans << endl;

	return 0;
}