program Project5;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var s,rez:string; d,i:integer;
begin
    readln(s);
    d:=length(s);
  for I:= 1 to d do
      if s[i]<>'#' then
       rez:=rez+s[i]
       else
       if length(rez)<>0 then
       delete (rez,length(rez), 1);
   write(rez);
   readln;
  { TODO -oUser -cConsole Main : Insert code here }
end.
