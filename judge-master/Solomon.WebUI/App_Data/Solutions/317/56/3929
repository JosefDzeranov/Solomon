var n,k,i,z:longint;
begin
 //  assign(input,'input1.txt');reset(input);
 //  assign(output,'output1.txt');rewrite(output);
   readln(n);
   k:=0;
   for i:=1 to n do
   begin
      z:=i;
      while z<>0 do
      begin
        if z mod 2=0 then
         z:=z div 2;
        k:=k+1;
      end;
   end;
   writeln(k);
end.
