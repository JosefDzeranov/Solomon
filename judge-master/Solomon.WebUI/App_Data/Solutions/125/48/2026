#include <iostream>

using namespace std;

int main()
{
    int a, b, c, d, e, f, g;
    cin>>a>>b>>c>>d>>e;
    if(b<0)
    b=b-b*(-2);
    if(c<0)
    c=c-c*(-2);
    if(d<0)
    d=d-d*(-2);
    if (e<0)
    {e=e-e*(-2);}
    f=b+d;
    g=c+e;
    if(a>f/2&&a>g/2)
    cout<<f*g;
    else
    cout<<a*a;
    return 0;
}
