var s:string;
n,k,i,l,l1:integer;

begin
readln(n,k);l:=0;l1:=0;
for i:=1 to n do 
begin
readln(s);
     if s='ammo' then begin l1:=l1+k;writeln(l1);end
else if s='save' then begin l:=l1; writeln(l1);end
else if s='load' then begin l1:=l; writeln(l1);end
else if s='shoot' then begin if l1>0 then l1:=l1-1;writeln(l1);end;
end;
end.
