var a,b,i,n:longint;
begin
read(b);
for i:=2 to b do if i mod 2 =0 then begin n:=i; 
                                          while n mod 2 = 0 do begin
                                                              a:=a+1;
                                                              n:=n div 2;
                                                              end;
                                    end;
write(a);
end.