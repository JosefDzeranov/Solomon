#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int a1,a2,a3,a4,n;
    cin >> a1 >> a2 >> a3 >> a4;
    n=0;
    if (a1<=a2)
    {
       n=n+a1;
    }
    else
    {
        n=n+a2;
    }
    if (a3<=a4)
    {
       n=n+a3;
    }
    else
    {
        n=n+a4;
    }
    if (round(sqrt(n))<=sqrt(n))
    {
        cout <<round(sqrt(n));
    }
    else
    {
        cout <<round(sqrt(n))-1;
    }
    return 0;
}

























