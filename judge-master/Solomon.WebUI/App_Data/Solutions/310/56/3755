#include <iostream>
#include <cstdio>
#include <cstdlib>
#include <vector>
#include <algorithm>

using namespace std;

int n,ans,d;

int main(){
    cin>>n;
    for(int i=2;i<=n;i+=2){
        d=i;
        while (d%2==0){
            d/=2;
            ans++;
        }
    }
    cout<<ans<<endl;

    return 0;
}
