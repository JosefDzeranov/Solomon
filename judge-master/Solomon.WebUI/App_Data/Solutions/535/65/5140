#include <iostream>
#include <fstream>
#include <string>
#include <math.h>
using namespace std;

int main()
{
    long long int a1,a2,a3,a4,b1,b2;
   long  K;
cin>>a1>>a2>>a3>>a4;
if(a1<a2 ||a2<a1 ||a3<a4 ||a4<a3 ){
if(a1<a2)
    b1=a1;
if(a2<a1)
    b1=a2;
    if(a3<a4)
    b2=a3;
if(a4<a3)
    b2=a4;}
    else{
b1=(a1+a2)/2;
b2=(a3+a4)/2;}
K=b1+b2;
K=sqrt(K);
cout<<K;
    return 0;
}
