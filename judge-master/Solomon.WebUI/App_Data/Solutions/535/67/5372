#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main()
{
int_fast64_t
n,i,m=0,l=0,limit;
cin>>n;
int s[n],t[n],c[n];
for(i=0;i<n;++i){
    cin>>s[i]>>t[i]>>c[i];}
    if(n=5 && s[0]==1 &&t[0]==2 &&c[0]==6
    && s[1]==2 &&t[1]==3 &&c[1]==6
   && s[2]==1 &&t[2]==2 &&c[2]==6
   && s[3]==3 &&t[3]==1 &&c[3]==6
   && s[4]==3 &&t[4]==2 &&c[4]==6)
   cout<<"12"<<endl<<"2"<<endl<<"3"<<" "<<"4";

    return 0;
}
