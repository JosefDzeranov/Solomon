using System;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            string s = Console.ReadLine();
            UInt64 sum = 0, st = 1;
            for (int i = s.Length-1; i >=0 ; i--)
            {
                if (s[i] == '*')
                    sum += st;
                st *= 2; 
            }
            Console.WriteLine(sum);
        }
    }
}
