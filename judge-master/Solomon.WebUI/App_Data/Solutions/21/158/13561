#include <iostream>
using namespace std;
#include <string>
int main(){
	string a;
	cin>>a;
	int rip=0;
	for (int i=0;i<a.length();i++)
	{
		if (a[i]=='v' && a[i+1]=='k' && a[i+2]=='o' && a[i+3]=='n' && a[i+4]=='t' && a[i+5]=='a' && a[i+6]=='k' && a[i+7]=='t' && a[i+8]=='e')
			for (int j = 0; j < a.length(); j++)
			{
				if (a[j]='a' && a[j+1]=='p' && a[j+2]=='i')
					rip++;

			}
	}
	cout<<rip;
	return 0;

}