#include <iostream>
#include <math.h>
#include <stdlib.h>
#include <algorithm>
using namespace std;

int main()
{
    long long int a1,a2,a3,a4,e,d;
    cin>>a1>>a2>>a3>>a4;
     e=min(a1,a2);
     d=min(a3,a4);
    e+=d;
    long double ee=e;
    int  ans=sqrt(ee);

    cout << ans;
    return 0;
}
