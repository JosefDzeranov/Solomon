program Project38;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var
  n, m,k: Integer;


begin
  readln(n, m);
  n:= N * n;
  k:=n-m;
     if m>n then
  writeln('NO') else
  if sqrt(k)=trunc(sqrt(k)) then
  writeln('YES')
else
  if m>n then
  writeln('NO') else
  if sqrt(m)=trunc(sqrt(m)) then
  writeln('YES')
  else  writeln('NO');
  readln;

end.
