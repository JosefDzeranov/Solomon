program Project2;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var n,l,r1,r2,x,y:integer;
begin
  readln(n,l);
  readln(r1,r2);
    x:=(l div (r1*2));
   if (r1*2*x)<l then x:=x+1;
   y:=x;
   x:=x*2;
  if (n-((r1*2*3.14*x)+(r2*2*3.14*y))>=0) then
  begin
  writeln('YES');
  writeln(x,' ',y);
  end
  else
  writeln('NO');
  readln;
end.
 