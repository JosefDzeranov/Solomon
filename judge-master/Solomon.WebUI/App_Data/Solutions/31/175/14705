program Project1;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var n, i, r, m, k: integer;
begin
  readln(n);
  n := n * 2 - 1;
  writeln('YES');
  for i := 1 to n do
   begin
     if n mod i = 0 then
      begin
        r := i;
        m := n div i;
        if r <= m then
        writeln(r,' ',m);
      end;
      
   end;
  readln;
end.

