program Project31;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x1,y1,x2,y2,t,x,y:integer;
begin
  readln(x1,y1,x2,y2);
  if x1<0 then x1:=abs(x1) else if x1>100 then x1:=x1-100;
  if x2<0 then x2:=abs(x2) else if x2>100 then x2:=x2-100;
  if y1<0 then y1:=abs(y1) else if y1>100 then y1:=y1-100;
  if y2<0 then y2:=abs(y2) else if y2>100 then y2:=y2-100;


  if x1>x2 then
  x:=x2*2+100 else x:=x1*2+100;

  if y1>y2 then
  y:=y2*2+100 else y:=y1*2+100;

  t:=(x+y)*5;

 writeln(t);
 readln;
end.
