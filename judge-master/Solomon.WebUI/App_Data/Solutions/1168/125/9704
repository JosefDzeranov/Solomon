#include<iostream>
#include<cmath>
using namespace std;
main()
{
      int i,j,x1,x2,x3,y1,y2,y3;
      cin>>x1>>y1>>x2>>y2>>x3>>y3;
      if(abs(x3-x1)>abs(x3-x2))i=x3-x2;
      if(abs(x3-x1)<abs(x3-x2))i=x3-x1;
      if(abs(y3-y1)>abs(y3-y2))j=y3-y2;
      if(abs(y3-y1)<abs(y3-y2))j=y3-y1;
      if(abs(j)>abs(i))cout<<abs(i);
      if (abs(j)<abs(i)) cout<<abs(j);
      }
