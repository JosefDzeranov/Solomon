#include<iostream>
#include<cmath>
using namespace std;
main()
{
      int A[1000],i,j,x1,x2,x3,y1,y2,y3;
      cin>>x1>>y1>>x2>>y2>>x3>>y3;
      if(x3-x1>x3-x2 && x3-x2>0 && x3-x1>0)i=x3-x2;
      else i=x3-x1;
      if(y3-y1>y3-y2 & y3-y2>0 && y3-y1>0)j=y3-y2;
      else j=y3-y1;
      if(j>i && i>0 && j>0)cout<<i;
      else cout<<j;
      }
