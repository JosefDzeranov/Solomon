#include<iostream>
#include<cmath>
using namespace std;
int main() {
	unsigned long long a1, a2, a3, a4, r;
	cin >> a1 >> a2 >> a3 >> a4;
	if (a1>a2)
		if (a3>a4)
			r=sqrt(a2+a4);
		else
			r=sqrt(a2+a3);
	else
		if (a3>a4)
			r=sqrt(a1+a4);
		else
			r=sqrt(a1+a3);
	cout << r;
	cin.ignore();
	cin.get();
	return 0;
}