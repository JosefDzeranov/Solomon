#include<iostream>
using namespace std;
#include<cmath>

int main() {
	double n1;
	int n, l, r, l1;
	cin >> n >> l >> r;
	n1=2*3.14*r;
	l1=r*2;
	for (int i=1; ;i++) {
		if(l<=l1*i) {
			if (n>=n1*i)
				cout << "YES\n" << i;
			else
				cout << "NO";
			break;
		}
	}
	cin.ignore();
	cin.get();
	return 0;
}