#include<iostream>
using namespace std;

int main(){
	int n,m,y=0,v,b;
	int k[10][10];
	cin>>n>>m;
	for(int i = 0; i < n; i++){
		for(int  j = 0; j < m; j++){
			cin>>k[i][j];
		}
	}
	for(int i = 0; i < n; i++){
		for(int j = 0; j < m; j++){
			if(k[i][j] == 1){
				if(j==n-1){
					y+=2;
				}					
			}
		}
	}
cout<<y;
	return 0;
}