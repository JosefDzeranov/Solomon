#include <iostream>
#include <math.h>

using namespace std;

int sqr(int n)
{
    return n * n;
}


int main()
{
    int x1, x2, x3;
    int y1, y2, y3;
    cin >> x1 >> y1;
    cin >> x2 >> y2;
    cin >> x3 >> y3;
    if (x3 >= x1 && y3 >= y1 && x3 <= x2 && y3 <= y2)
        cout << min(min(fabs(x3 - x1), fabs(x3 - x2)), min(fabs(y3 - y1), fabs(y3 - y2)));

    else if (x3 >= x1 && x3 <= x2)
    {
        if (y1 > y3)
            cout << fabs(y3 - y1);

        else
            cout << fabs(y3 - y2);
    }

    else if (y3 >= y1 && y3 <= y2)
    {
        if (x1 > x3)
            cout << fabs(x3 - x1);

        else
            cout << fabs(x3 - x2);
    }
    //______________________

    else if (y3 <= y1)
    {
        if (x3 < x1)
            cout << sqrt(double(sqr(fabs(y3 - y1)) + sqr(fabs(x3 - x1))));

        else
            cout << sqrt(double(sqr(fabs(y3 - y1)) + sqr(fabs(x3 - x2))));

    }

    else if (y3 >= y2)
    {
        if (x3 < x1)
            cout << sqrt(double(sqr(fabs(y3 - y2)) + sqr(fabs(x3 - x1))));

        else
            cout << sqrt(double(sqr(fabs(y3 - y2)) + sqr(fabs(x3 - x2))));
    }

    return 0;
}
