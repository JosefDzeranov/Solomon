program Project1;

{$APPTYPE CONSOLE}

uses
  SysUtils;
var x:array[1..4] of longword;
z:longword;
i:integer;
begin
for i:=1 to 4 do
read(x[i]);
z:=0;
while (x[1]>0)and (x[2]>0) do begin
x[1]:=x[1]-1;
x[2]:=x[2]-1;
z:=z+1;
end;
while(x[3]>0) and(x[4]>0) do begin
x[3]:=x[3]-1;
x[4]:=x[4]-1;
z:=z+1;
end;
z:=trunc(sqrt(z));
writeln(z);
readln;
readln;
end.
