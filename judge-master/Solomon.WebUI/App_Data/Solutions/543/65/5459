program Project2;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var x:array[1..4] of int64;
z,i:integer;
begin
for i:=1 to 4 do
read(x[i]);
if x[1]<x[2] then
z:=x[1]
else z:=x[2];
if x[3]<x[4] then
z:=z+x[3]
else z:=z+x[4];
z:=trunc(sqrt(z));
write(z);
readln;
readln; { TODO -oUser -cConsole Main : Insert code here }
end.
 