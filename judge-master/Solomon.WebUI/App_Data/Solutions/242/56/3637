using System;
using System.Collections.Generic;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int k = 0, q;
            for (int i = 1; i <= n; i++)
            {
                q = i;
                while (q % 2 == 0) { q /= 2; k++; }
            }
            Console.WriteLine(k);
        }
    }
}
