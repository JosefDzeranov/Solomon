program Project1;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var mas:array [1..10000] of string;
a,b,c,i,r:integer;

begin
  readln(c);
   for i := 1 to  c do
   begin
   readln(mas[a]);
   end;
   readln;
   a:=0;
     for r := 1 to c do
  begin
  if mas[r]=mas[r+1] then a:=a+1




  end;
          writeln(a);

  readln;







end.
