#include <iostream>
#include <string>

using namespace std;

int main()
{
	int n;
	cin >>n;
	int ans = 0;
	string num;
	getline(cin, num);
	for (int i = 0; i < n; i++)
	{
		getline(cin, num);
		string nn = "";
		
		for (unsigned int j = 0; j < num.size(); j++)
		{
			while ((num[j] < '0' || num[j] > '9') && (j < num.size())) 
				j++;

			while (j < num.size() && num[j] >= '0' && num[j] <= '9')
			{
				nn += num[j];
				j++;
			}
			int a = stoi(nn);
			ans += a;
		}
	
	}
	cout << ans;
	return 0;
}