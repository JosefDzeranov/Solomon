#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    long double a, b, c, d;
    cin >> a >> b >> c >> d;
    long double t1 = min(a, b);
    long double t2 = min(c, d);
    t1 += t2;
    long double tt = t1;
    long long int res = sqrt(tt);
    cout << res;
    return 0;
}
