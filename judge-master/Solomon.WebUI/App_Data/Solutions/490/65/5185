#include<iostream>
#include<math.h>
using namespace std;
int main(){
int a,b,c,d;

if(a != 0 ||b != 0 ||c != 0 ||d != 0 ){
cin>>a>>b>>c>>d;
if(a > b)
	a = a-(a-b);
else
	b = a-(b-a);
if(c > d)
	c = c-(c-d);
else
	d = d-(d-c);
int g=0,f=0;
g = (a+b+c+d)/2;)
if(g>0){
if(g != 2){
cout << int(sqrt(g));
/}
else cout<<'0';}
else
    cout<<'0';}
    else cout <<'0';
return 0;
}
