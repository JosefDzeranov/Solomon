#include <iostream>

using namespace std;

int main()
{int n,m;
cin>>n;
int a[n],i,b[m][2],j;
float s;
for(i=0;i<n;i++)
    cin>>a[i];
cin>>m;
for(i=0;i<m;i++)
{
    cin>>b[i][0];
    cin>>b[i][1];
}
for(i=0;i<n;i++)
{
    for(j=0;j<m;j++)
    {
        if(a[i]==b[j][0]){s=s+b[j][1];}
    }
}
cout<<s;
    return 0;
}
