#include <iostream>
#include <stdio.h>

using namespace std;

int main () {
	int n;

	freopen("input.txt", "r", stdin);
	//freopen("relatives.out", "w", stdout);
	
	long long int mas[1025];
	for (int i = 0; i < 1025; i++)
		mas[i] = 0;

	long long int ans = 0;
	int a;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> a;
		int number = 0;
		if (!a)
			mas[0]++;
		else
			while (a) {
				number = number | (1 << (a % 10));
				a /= 10;
			}
		mas[number]++;
	}

	for (int i = 0; i < 1025; i++)
	{
		for (int j = i; j < 1025; j++)
			if (i & j)
				if (i != j)
					ans += mas[i] * mas[j];
				else
					ans += mas[i] * (mas[j] - 1) / 2;
	}
	cout << ans << endl;
	
	return 0;
}
