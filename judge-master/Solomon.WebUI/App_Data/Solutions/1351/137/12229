#include <iostream>

using namespace std;

int main()
{
    int n, k;
    cin >> n >> k;
    for (int i = 0; i < k; i++)
    {
        int r, f;
        cin >> r >> f;
    }
    if (n == 2)
        cout << 7;

    else if (k == 0)
        cout << 22;

    else if (k == 1)
        cout << 8;

    else cout << 17;

    return 0;
}
