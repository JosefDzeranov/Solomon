#include <iostream>

using namespace std;

int main()
{
    long long a, b, c, d, e;
    cin >> a >> b >> c >> d >> e;
    long long ans = e / (a - a / b + c - c / d);
    ans -= 4;
    if (ans > 100000000000)
    {
        while ((ans / 2) * a  - (ans / b / 2) * a  + (ans / 2) * c  - (ans / d / 2) * c  <= e / 2)
            ans++;

        return 0;
    }

    while (a * (ans - ans / b) + c * (ans - ans / d) < e)
        ans++;

    cout << ans;
    return 0;
}

//2 1000000000000 3 1000000000000 25;

//a * (ans - ans / b) + c * (ans - ans / d) < e
