#include<iostream>
using namespace  std;
int main ()
{
	int n=1, c=0, h=0;
	int o,d;
	cin>>n;
	int* a = new int[n];
	for(int i=0; i<n; i++)
		cin>>a[i];
	for(int j=0; j<n;j++)
		c=c+a[j];


	d=c/n;
	o=c%n;
	if(o==0){for(int k = 0; k<n; k++){
		if(a[k]<d || a[k]>d)h++;}cout<<h;}
	else cout<<"IMPOSSIBLE";
	}