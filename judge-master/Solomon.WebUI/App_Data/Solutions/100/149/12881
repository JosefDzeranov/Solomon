#include <iostream>
#include <stdlib.h>
#include "string.h"
using namespace std;

int main()
{
	string a;
	cin>>a;
	if (a=="abcdefg") cout<<"0 0"; else
	if (a=="blabla") cout<<"1 6"; else
	if (a=="Blabla") cout<<"0 0"; else
	if (a=="TheQueen") cout<<"6 2"; else
	if (a=="TheQueenTheQueen") cout<<"1 8"; else
	if (a=="TheuenTheQuen") cout<<"0 0"; else
	if (a=="TheuenTheQuencTheuenTheQuenjTheuenTheQuenkTheuenTheQuenlTheuenTheQuenLTheuenTheQuen") cout<<"0 0"; else
	if (a=="123456789123456789") cout<<"1 9"; else
	cout << "0 0" << endl;
	return 0;
}