#include <iostream>

using namespace std;

bool IsPrime(int num){
	for (int i = 2; i <= num / 2; i++){
		if (num%i == 0) return false;
	}
	return num >= 2;
}

int main()
{
	int c[100] = { 0 };
	int v[100] = { 0 };
	int d[100] = { 0 };
	int i, n, k, max;
	cin >> n;

	for (i = 0; i < n; i++)//считываем элементы
		cin >> c[i];

	for (i = 0; i < n; i++)//считаем разрядности
	{
		k = c[i];
		while (k>0)
		{
			k /= 10;
			v[i]++;
		}
		k = v[i];
		d[k]++;
	}

	max = 0;
	k = d[0];
	for (i = 0; i < 9; i++)//находим максимлаьную разрядность
	{
		if (d[i]>k) { max = i; k = d[i]; }
	}

	for (i = 0; i < n; i++)//выводим ответ (ближайшее простое ищем сразу же)
	{
		if (v[i] == max)
		{
			while (!IsPrime(c[i]+1)) c[i]++;
			c[i]++;
		}
	}
		
	for (i = 0; i < n; i++)
	{
		cout << c[i] << " ";
	}
	return 0;
}

