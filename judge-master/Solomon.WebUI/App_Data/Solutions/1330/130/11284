#include <iostream>
#include <string>
using namespace std;

int main()
{
    int a,b,c,d,z=0;
    cin>>a>>b>>c>>d;
    for (int i=0; i<d-(d/2); i++)
    {
        for (int j=0;  j<d-(d/2) ; j++)
        {
           if (i*j<=b && i*j>=a && i+j>=c && i+j<=d)
            z++;
        }

    }
    cout<<z/2;
    return 0;
}
