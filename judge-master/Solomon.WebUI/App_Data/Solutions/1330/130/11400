#include <iostream>

using namespace std;

int main()
{
    long long a,b,c,d,z=0;
    cin>>a>>b>>c>>d;
    for (int i=0; i<=d-(d/2); i++)
    {
        for (int j=0;  j<=d-(d/2) ; j++)
        {
           if (i*j<=b && i*j>=a && 2*(i+j)>=c && 2*(i+j)<=d)
            z++;
        }

    }
    if (z%2==0)
    cout<<z/2;
    else
        cout<<z/2+1;
    return 0;
}
