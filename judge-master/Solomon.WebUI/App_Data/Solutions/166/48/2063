#include <iostream>

using namespace std;

int main()
{
    int l,x1,x2,x3,x4,y1,y2,y3,y4,m,n,s;
    cin>>l>>x1>>y1>>x2>>y2;
    x3=x2;
    y3=y1;
    x4=x1;
    y4=y2;
    m=x4-x1+y4-y1;
    n=x3-x1+y3-y1;
    if (l>m && l>n)
    s=n*m;
    if (l<m && l<n)
    s=2*3.14*l;
    cout<<s;
    return 0;
}
